@using Web.Models
@{
    LayoutModel myModel = ViewBag.LayoutModel;
    ViewBag.Title = "Schedule";
}



<div class="container">

    <h1 class="text-center">Schedule</h1>
    <div class="row">
        <div class="col-sm-12">

            @if (myModel.ZenPlannerName != "")
            {
                @Html.ZenCalendar(myModel.ZenPlannerName)
                @Html.ZenCalendarJS(myModel.ZenPlannerName)
            }
            else
            {
                <h1>Your calendar is not set up!  Please do so!</h1>
            }



        </div>
    </div>
</div>

